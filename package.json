{
  "name": "envcoder",
  "version": "1.0.9",
  "description": "Package, that allows you to encrypt, decrypt, convert your .json to .env file",
  "keywords": [
    "env",
    "convert",
    "url",
    "decrypt",
    "encrypt"
  ],
  "main": "./bin/cli.js",
  "bin": {
    "envcoder": "./bin/cli.js"
  },
  "scripts": {
    "prebuild": "rimraf bin",
    "build": "yarn prebuild && tsc -p . && chmod u+x ./bin/cli.js",
    "publish-package": "npm run build && npm publish",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurborCorp/envcoder.git"
  },
  "author": "Roman Cold",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TurborCorp/envcoder/issues"
  },
  "homepage": "https://github.com/TurborCorp/envcoder#readme",
  "dependencies": {
    "cli-color": "^2.0.3",
    "commander": "^9.4.1",
    "crypto-js": "^4.1.1",
    "node-fetch": "^2.6.7",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.11.4",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.13",
    "husky": "^8.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
